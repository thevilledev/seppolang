fn seppo() {
    x = 42
    seppo x

    // seppo as a conditional block (if-else)
    y = 0
    seppo x > 40 {
        y = 10 - 20
    }
    perkele {
        y = 5 - 10
    }
    seppo y

    // nested conditional blocks
    z = 0
    seppo y < 30 {
        z = 100
        seppo z != 100 {
            seppo z
        }
        perkele {
            z=3
        }
    }
    perkele {
        z = 0
    }
    seppo z
}
